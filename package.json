{
  "name": "ublacklist",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#builtin/*": "./builtin/*"
  },
  "scripts": {
    "build": "node scripts/build.ts",
    "check": "pnpm run /^check:/",
    "check:biome": "biome check",
    "check:prettier": "prettier --check **/*.{md,yaml,yml}",
    "check:typescript": "tsgo",
    "fix": "pnpm run /^fix:/",
    "fix:biome": "biome check --write",
    "fix:prettier": "prettier --write **/*.{md,yaml,yml}",
    "generate": "pnpm run /^generate:/",
    "generate:message-names": "node scripts/generate-message-names.ts",
    "generate:ruleset-parser": "lezer-generator src/scripts/ruleset/ruleset.grammar -o src/scripts/ruleset/parser.generated.ts --noTerms --typeScript",
    "generate:third-party-notices": "node scripts/generate-third-party-notices.ts",
    "test": "node scripts/test.ts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.2",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.12.1",
    "@codemirror/lint": "^6.9.3",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.12",
    "@lezer/common": "^1.5.1",
    "@lezer/highlight": "^1.2.3",
    "@lezer/lr": "^1.4.8",
    "@mdi/svg": "^7.4.47",
    "@neodrag/vanilla": "^2.3.1",
    "@sindresorhus/fnv1a": "^3.1.0",
    "colord": "^2.9.3",
    "css-tree": "^3.1.0",
    "dayjs": "^1.11.19",
    "es-toolkit": "^1.44.0",
    "goober": "2.1.15",
    "is-mobile": "^5.0.0",
    "js-yaml": "^4.1.1",
    "lz-string": "^1.5.0",
    "punycode": "^2.3.1",
    "react": "^19.2.4",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.4",
    "tldts": "^7.0.22",
    "webdav": "^5.9.0",
    "zod": "^4.3.6",
    "zod-validation-error": "5.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@lezer/generator": "^1.8.0",
    "@types/css-tree": "^2.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/license-checker": "^25.0.6",
    "@types/node": "^24.10.11",
    "@types/punycode": "^2.1.4",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript/native-preview": "7.0.0-dev.20260206.1",
    "csstype": "^3.2.3",
    "dotenv": "^17.2.4",
    "esbuild": "^0.27.3",
    "fs-extra": "^11.3.3",
    "lefthook": "^2.1.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "semantic-release-amo": "^2.0.1",
    "semantic-release-chrome": "^3.2.0"
  },
  "packageManager": "pnpm@10.28.2",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.13.0",
      "onFail": "download"
    }
  }
}
