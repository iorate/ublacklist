    "tldts": "^6.1.18",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
    "@types/license-checker": "^25.0.6",
    "@types/node": "^20.12.7",
    "@types/punycode": "^2.1.4",
    "@types/react": "^18.2.79",
