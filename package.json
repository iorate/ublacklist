{
  "name": "ublacklist",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/build.ts",
    "check": "pnpm run /^check:/",
    "check:biome": "biome check",
    "check:prettier": "prettier --check **/*.{html,yml,yaml,md}",
    "check:tsc": "tsc --build",
    "fix": "pnpm run /^fix:/",
    "fix:biome": "biome check --write",
    "fix:prettier": "prettier --write **/*.{html,yml,yaml,md}",
    "generate": "pnpm run /^generate:/",
    "generate:ruleset-parser": "lezer-generator src/scripts/ruleset/ruleset.grammar -o src/scripts/ruleset/parser.ts --noTerms --typeScript",
    "generate:third-party-notices": "node scripts/generate-third-party-notices.ts",
    "test": "node --test src/**/*.test.ts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.2",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.0",
    "@lezer/common": "^1.2.3",
    "@lezer/highlight": "^1.2.1",
    "@lezer/lr": "^1.4.2",
    "@mdi/svg": "^7.4.47",
    "@sindresorhus/string-hash": "^2.0.0",
    "colord": "^2.9.3",
    "css-tree": "^3.1.0",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.39.5",
    "goober": "^2.1.16",
    "is-mobile": "^5.0.0",
    "js-yaml": "^4.1.0",
    "punycode": "^2.3.1",
    "react": "^19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "tldts": "^7.0.9",
    "zod": "^3.25.67",
    "zod-tuple-with-optional": "^1.0.2",
    "zod-validation-error": "^3.5.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@lezer/generator": "^1.8.0",
    "@types/css-tree": "^2.3.10",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/license-checker": "^25.0.6",
    "@types/node": "^24.0.7",
    "@types/punycode": "^2.1.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/webextension-polyfill": "^0.12.3",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "csstype": "^3.1.3",
    "dotenv": "^17.0.0",
    "esbuild": "^0.25.5",
    "fs-extra": "^11.3.0",
    "globby": "^14.1.0",
    "lefthook": "^1.11.14",
    "license-checker": "^25.0.1",
    "p-limit": "^6.2.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.5",
    "semantic-release-amo": "^2.0.0",
    "semantic-release-chrome": "^3.2.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "pnpm": ">=10"
  }
}
