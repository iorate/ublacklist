{
  "name": "ublacklist-website",
  "version": "0.0.0",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.15",
    "@docusaurus/types": "2.0.0-beta.15",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react-router-dom": "^5.3.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,mdx}": "prettier --write"
  },
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "clear": "docusaurus clear",
    "lint": "FORCE_COLOR=1 run-p -l lint-eslint lint-prettier lint-typecheck",
    "lint-eslint": "yarn g:eslint .",
    "lint-prettier": "yarn g:prettier --check --loglevel warn .",
    "lint-typecheck": "tsc",
    "fix": "yarn fix-prettier",
    "fix-prettier": "yarn g:prettier --write --loglevel warn ."
  }
}
